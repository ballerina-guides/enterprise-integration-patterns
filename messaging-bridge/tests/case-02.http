POST http://localhost:8080/api/v1
content-type: application/json
X-REQUEST-TYPE: GraphQL

mutation ($orgID: String!, $projInput: ProjectInput!) {
  createProject(organizationID: $orgID, projectInput: $projInput) {
    projectName,
    customerName,
    description,
    tasks {
      taskID,
      description
    }
  }
}

